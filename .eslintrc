{
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "semi": 2,
    "arrow-body-style": [
      2,
      "always"
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "object-curly-spacing": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "consistent-return": 0
  },
  "extends": "airbnb/base"
}
